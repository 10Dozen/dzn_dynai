<!-- Zone's properties after zone initialization -->

<name id="0"><!-- Zone name (Optional) --></name>
<side id="1"><!-- Side --></side>
<isActive id="2"><!-- is zone active (bool) --></isActive>
<area id="3">
	<location id="0"><!-- createLoction => location --></location>
	<location id="n"><!-- createLoction => location --></location>
</area>
<wp id="4">
	<wpPos id="0"><!-- getWPPos => [Pos3D] --></wpPos>
	<wpPos id="n"><!-- getWPPos => [Pos3D] --></wpPos>
	<generate><!-- BOOLEAN --></generate> <!-- Нужно ли генерировать дополнительные чекпоинты -->
</wp>
<refUnits id="5">
	<refTemplate id="0">
		<count><!-- getDir leader --></count>
		<refGroup>
			<refUnit id="0">
				<classname><!-- Classname --></classname>
				<assignedTo><!-- :isVehicle" for vehicle unit --></assignedTo>
				<gear><!-- Gear String --></gear>
			</refUnit>
			<refUnit id="1">
				<classname><!-- Classname --></classname>
				<assignedTo>
					<vehicle><!-- Unit Id inside Group --></vehicle> 
					<role><!-- Role --></role>
				</assignedTo>
				<gear><!-- Gear String --></gear>
			</refUnit>
			<refUnit id="2">
				<classname><!-- Classname --></classname>
				<assignedTo><!-- Empty array - no assignement--></assignedTo>
				<gear><!-- Gear String --></gear>
			</refUnit>
			<refUnits id="n"><!-- ... --></refUnits>
		</refGroup>
		<customSkillLevel>
			<skillType><!-- Simple or Complex --></skillType>
			<complexSkill id="">
				<!-- Value or Array of skill values -->
			</complexSkill>
		</customSkillLevel>
		
		
	</refTemplate>
	<refTemplate id="1">
		<count><!-- getDir leader --></count>
		<refGroup>
			<refUnit id="0">
				<classname><!-- Classname --></classname>
				<assignedTo>
					<inBuilding><!-- "inBuilding" for spawn inside houses --></inBuilding>
					<buildingList><!-- List of buildings to use. If empty - default is used --></buildingList>
				</assignedTo>
				<gear><!-- Gear String --></gear>
			</refUnit>
			<refUnits id="n"><!-- ... --></refUnits>
		</refGroup>
	</refTemplate>
	<refTemplate id="n"><!-- ... --></refTemplate>
</refUnits>
<behavior id="6">
	<speedMode><!-- wpSpeedMode --></speedMode>
	<behavior><!-- wpBehavior --></behavior>
	<combatMode><!-- wpCombatMode --></combatMode>
	<formation><!-- wpFormation --></formation>
	<courage><!-- wpDescription."courage" --></courage>
</behavior>
<zoneBuildings id="7"><!-- List of objects --></zoneBuildings>

